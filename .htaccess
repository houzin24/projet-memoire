RewriteRule ^emploi_du_temps$ emploi_du_temps.php
RewriteRule ^annee$ annee.php
RewriteRule ^payement$ payement.php
RewriteRule ^activite$ activite.php
RewriteRule ^ajout_activite$ ajout_activite.php
RewriteRule ^ajout_participant$ ajout_participant.php
RewriteRule ^salle$ salle.php
RewriteRule ^ajout_salle$ ajout_salle.php
RewriteRule ^absence$ absence.php
RewriteRule ^ajout_absence$ ajout_absence.php
