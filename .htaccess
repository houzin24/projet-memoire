RewriteRule ^ajout_etudiant$ ajout_etudiant.php
RewriteRule ^professeur$ professeur.php
RewriteRule ^ajout_professeur$ ajout_professeur.php
RewriteRule ^parent$ parent.php
RewriteRule ^personnel$ personnel.php
RewriteRule ^ajout_personnel$ ajout_personnel.php
RewriteRule ^niveau$ niveau.php
